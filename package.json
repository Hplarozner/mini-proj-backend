{
  "name": "backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "copy-files": "copyfiles -u 1 ./src/**/*.json ./dist",
    "start": "node --require dotenv/config ./dist/app.js",
    "build": "npm install --production=false && npm run deploy",
    "deploy": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json && npm run copy-files",
    "build:watch": "yarn clean && tsc && yarn copy-files && (concurrently \"tsc -w\" \"tsc-alias -w\")",
    "dev": "cross-env NODE_ENV=development nodemon --require dotenv/config ./src/app.ts"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@datastax/astra-db-ts": "^1.5.0",
    "@langchain/community": "^0.3.37",
    "@langchain/core": "^0.3.43",
    "@langchain/openai": "^0.5.0",
    "@types/mongoose": "^5.11.96",
    "ai": "^4.2.5",
    "body-parser": "^1.20.3",
    "chrome": "^0.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "install": "^0.13.0",
    "joi": "^17.13.3",
    "langchain": "^0.3.19",
    "mongoose": "^8.13.0",
    "morgan": "^1.10.0",
    "openai": "^4.89.0",
    "puppeteer": "^24.4.0",
    "whatwg-url": "^14.2.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.13.13",
    "concurrently": "^9.1.2",
    "copyfiles": "^2.4.1",
    "eslint": "^9.23.0",
    "nodemon": "^3.1.9",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.11",
    "typescript": "^5.8.2"
  }
}
